{
  "name": "paymentcenter",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "server": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node utils/seedLocalDB.js",
    "oldbuild": "cd client && npm run build",
    "build": "npm install --prefix ./client && npm run build --prefix ./client && npm install && npm run seed",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.18",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.3",
    "bootstrap": "^5.3.3",
    "concurrently": "^8.2.2",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "gh-pages": "^6.1.1",
    "if-env": "^1.0.4",
    "moment": "^2.30.1",
    "mongoose": "^8.2.1",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.2",
    "react-scripts": "^3.0.1"
  }
}
